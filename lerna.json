{
  "version": "0.0.1",
  "packages": [
    "packages/*"
  ],
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md",
        "*.txt",
        "test/**",
        "**/test/**"
      ]
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    }
  }
}
